{
  "name": "mapgo-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/app.js",
  "scripts": {
    "start": "tsc && node --max-old-space-size=8192 dist/src/app.js",
    "build": "npm test && tsc",
    "pretest": "tsc",
    "test": "nyc mocha",
    "upload-codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "genDoc": "typedoc --module commonjs --target ES6 --out doc/typedoc src/",
    "build-container": "docker build -t alehuo/mapgo-backend .",
    "run-container": "docker run -p 8081:8081 -d alehuo/mapgo-backend",
    "publish-container": "docker push alehuo/mapgo-backend"
  },
  "keywords": [
    "Visualizer",
    "Algorithm",
    "Dijkstra",
    "BFS",
    "AStar",
    "TypeScript"
  ],
  "author": "Aleksi Huotala",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.49",
    "assert": "^1.4.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "mocha": "^4.0.1",
    "mocha-typescript": "^1.1.12",
    "nyc": "^11.2.1",
    "source-map-support": "^0.5.0",
    "ts-node": "^3.3.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "@types/socket.io": "^1.4.31",
    "dotenv": "^4.0.0",
    "mocha": "^4.0.1",
    "mocha-typescript": "^1.1.12",
    "socket.io": "^2.0.4",
    "typescript": "^2.5.3"
  }
}
